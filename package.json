{
  "name": "formula-parser",
  "version": "1.0.0",
  "description": "Formula expression parser and validation library.",
  "main": "index.html",
  "scripts": {
    "test": "nyc mocha"
  },
  "nyc": {
    "all": true,
    "extension": [
      ".ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "includes": [
      "src/**/!(*.spec).ts"
    ],
    "exclude": [
      "**/*.d.ts"
    ],
    "reporter": [
      "lcov",
      "json"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/KennethanCeyer/FormulaParser.git"
  },
  "keywords": [
    "Formula",
    "Parser",
    "VanilaJS",
    "FormulaBuilder",
    "Validator"
  ],
  "author": "PIGNOSE",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/KennethanCeyer/FormulaParser/issues"
  },
  "homepage": "https://github.com/KennethanCeyer/FormulaParser#readme",
  "devDependencies": {
    "@types/chai": "^4.1.2",
    "@types/mocha": "^5.0.0",
    "chai": "^4.1.2",
    "mocha": "^5.0.5",
    "nyc": "^11.6.0",
    "ts-lint": "^4.5.1",
    "ts-node": "^5.0.1",
    "tslint": "^5.9.1",
    "tslint-config-airbnb": "^5.8.0",
    "typescript": "^2.7.2"
  }
}
